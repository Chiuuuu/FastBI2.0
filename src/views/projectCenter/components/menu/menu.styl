.menu {
    display flex
    flex-direction column
    height 100%
    // min-height: 804px;
    background-color: #ffffff;
    .menu_title {
        font-size: 16px;
        font-family: Microsoft YaHei;
        font-weight: 400;
        color: rgba(1, 4, 15, 1);
        line-height: 48px;
        margin-left: 14px;
        border-bottom: 1px solid rgba(239, 239, 239, 1);
        display: flex;
        justify-content: space-between;
        .m-t-s {
          font-size: 17px;
          font-family: Microsoft YaHei;
          font-weight: 500;
          color: #020510;
          line-height: 48px;
        }
        .menu_icon {
            font-size: 18px;
            margin-right: 14px;
            margin-top: 16px;
            color: #627CFF;
        }
    }
    .menu_search {
        height: 32px;
        margin-left: 14px;
        margin-right: 4px;
        margin-top: 17px;
        margin-bottom: 17px;
        font-size: 14px;
        .icon_search {
          color: #627CFF;
        }
    }
    .table_list-_empty{
        color: rgba(0, 0, 0, 0.65);
        margin-top: 50px;
    }
}
.card-img{
    height 100px;
}
.card-title {
    color: #333;
    text-align: center;
    text-transform: capitalize;
}