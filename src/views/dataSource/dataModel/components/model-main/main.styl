.model-main{
        position: relative;
        height: 100%;
        // overflow-y: auto;
        background-color: #ffffff;
        width: 79%;
        .main-box {
            height 100%    
        }
        /deep/ .ant-spin-container {
            height 100%
        }
        .header {
            display: flex;
            justify-content: space-between;
            .data_con {
                overflow: hidden;
                text-overflow: ellipsis;
                white-space: nowrap;
                width: 350px;
                height: 40px;
                font-size: 16px;
                font-family: Microsoft YaHei;
                font-weight: bold;
                color: rgba(1, 4, 15, 1);
                line-height: 48px;
                margin-left: 21px;
            }
            .data_btn {
                width: 188px;
                display: flex;
                justify-content: space-between;
                margin-top: 9px;
                margin-right: 20px;
            }
        }
        .scrollable {
            width 100%
            height calc(100% - 50px)
        }
        .description {
            padding-left: 20px;
            margin-top: 10px;
            border-bottom: 1px solid #e8e8e8;
            .d-s{
                font-size:14px;
                font-family:Microsoft YaHei;
                font-weight:400;
                color:rgba(59,60,67,1);
                white-space: nowrap;
                text-overflow: ellipsis;
                overflow: hidden;
                max-width: calc(100% - 35px);
                display: inline-block;
                height: 20px;
                line-height: 20px;
            }
        }
        .tips {
            color: #ff0000;
            padding: 14px 21px 0;
            margin: 0;
            i {
                color: #FFC34F;
            }
        }
        .detail_header {
            display: flex;
            justify-content: space-between;
            margin-left: 21px;
            height: 50px;
            font-size: 16px;
            font-family: Microsoft YaHei;
            font-weight: 400;
            color: rgba(1, 4, 15, 1);
            .detail_btn {
                display: flex;
                justify-content: space-between;
                // width: 215px;
                margin-right: 20px;
            }
        }
        .detail_main {
            display: flex;
            justify-content: space-around;
            height: calc(100% - 50px);

            .line-through {
                text-decoration: line-through;
                color: #bbb 
                &:hover {
                    color: #bbb    
                }
            }
            
            .dim_title, .mea_title {
                height: 38px    
            }
            .dim_menu, .mea_menu {
                clear: both;
                height: calc(100% - 38px);
                background-color #fff;
                overflow-y: auto;
            }
            .dimensionality {
                margin-left: 21px;
                width: 50%;
                height: 100%;
                background: rgba(248, 248, 248, 1);
                border-bottom: 1px solid #e8e8e8;
                .dim_span {
                    font-size:14px;
                    font-family:Microsoft YaHei;
                    font-weight:400;
                    color:rgba(1,4,15,1);
                    line-height:38px;
                    margin-left: 40px;
                }
            }
            .measurement {
                margin-right: 21px;
                width: 50%;
                height: 100%;
                background: rgba(248, 248, 248, 1);
                border-bottom: 1px solid #e8e8e8;
                .mea_span {
                    font-size:14px;
                    font-family:Microsoft YaHei;
                    font-weight:400;
                    color:rgba(1,4,15,1);
                    line-height:38px;
                    margin-left: 50px;
                }
            }
        }
    }

.ant-col-4 {
    text-align: left;
}

.ant-form-explain {
    float: left;
}
.draw_board {
    height: calc(50% - 60px);
    overflow: auto;
    border-bottom: 1px solid #efefef;
}
.m-map {
    position: relative;
    box-sizing: border-box;
    padding: 14px 21px;
    height: 100%;
    width: 100%;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    -o-user-select: none;
    user-select: none;
    // border: 10px solid #f6f6f6;
}
.m-map-hover {
    border: 2px solid #90b9e8;
}
.main-empty{
    position: absolute;
    margin: 0 auto;
    left: 0;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
    color: rgba(0, 0, 0, 0.65);
}
.detail {
    height: calc(50% - 60px);
    width: 100%;
    margin-top: 14px;
}

.ant-collapse-borderless > .ant-collapse-item > .ant-collapse-content > .ant-collapse-content-box {
    padding: 0;
}
.u-bitem {
    line-height: 24px
    position: relative;
    height: 28px;
    overflow: hidden;
    word-wrap: normal;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding: 0 16px;
    &.edit {
        cursor: move;
        cursor: -webkit-grab;
        cursor: -moz-grab;
        cursor: -ms-grab;
        cursor: -o-grab;
    }
    .txt {
        line-height: 28px
        .icon {
            float: left;
            width: 14px;
            height: 14px;
            margin-top: 7px;
        }
        .name {
            display: block;
            overflow: hidden;
            word-wrap: normal;
            white-space: nowrap;
            text-overflow: ellipsis;
            -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
            -o-user-select: none;
            user-select: none;
            float: left;
            height: 28px;
            line-height: 28px;
            margin-left: 9px;
            width: calc(100% - 46px);
        }
        img {
            display: block;
            width: 100%;
            height: 100%;
        }
    }
}